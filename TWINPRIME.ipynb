    "def prime(i):\n",
    "\t\"\"\" This function finds whether a number is prime or not?\"\"\"\n",
    "\tval=False\n",
    "\tfor n in range(2,i):\n",
    "\t\tif i%n==0:\n",
    "\t\t\t\tbreak;\n",
    "\telse:\n",
    "\t    val=True\n",
    "\n",
    "\n",
    "\treturn val\n",
    "for x in range(2,1000):\n",
    "\tif prime(x)==True & prime(x+2)==True:\n",
    "\t\tprint(\"{} and {} are prime twins\".format(x,x+2))"
   ]
